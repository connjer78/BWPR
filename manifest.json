{
    "name": "BWPR Sprint Power-Up",
    "description": "A Power-Up to create sprint cards from templates based on labels.",
    "author": "Jeremy Conner",
    "version": "1.0.2",
    "icon": {
        "url": "https://connjer78.github.io/BWPR/assets/rocketship.PNG"
    },
    "capabilities": [
        "card-buttons"
    ],
    "card-buttons": [{
        "title": "Sprint It!",
        "icon": {
            "dark": "https://connjer78.github.io/BWPR/assets/rocketship.PNG",
            "light": "https://connjer78.github.io/BWPR/assets/rocketship.PNG"
        },
        "callback": "./index.html"
    }],
    "connectors": {
        "iframe": {
            "url": "./index.html"
        }
    },
    "permissions": [
        "read",
        "write"
    ]
}
